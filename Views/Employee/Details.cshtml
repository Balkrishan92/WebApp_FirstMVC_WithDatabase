@model WebApp_FirstMVC_WithDatabase.Models.Employee
<h2 class="text-info text-center">Employee Details</h2>
<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Name</td>
            <td>@Model.Address</td>
            <td>@Model.Salary</td>
        </tr>
    </tbody>
</table>
@Html.ActionLink("Edit", "Edit", new { id = Model.Id }, new { @class = "btn btn-info" })
@Html.ActionLink("Back To List", "Index", new { },new { @class = "btn btn-info" })